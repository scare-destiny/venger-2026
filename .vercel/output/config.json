{
	"version": 3,
	"routes": [
		{
			"src": "^/blog/analytical-tools$",
			"headers": {
				"Location": "/posts/posthog-vs-clarity-and-hotjar"
			},
			"status": 301
		},
		{
			"src": "^/blog/data-engineering-primer-part-1$",
			"headers": {
				"Location": "/posts/data-engineering-primer-part-1"
			},
			"status": 301
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/likes/add/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/likes/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}